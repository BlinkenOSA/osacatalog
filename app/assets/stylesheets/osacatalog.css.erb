body {
    color: #555 !important;
}

h1, h2, h3, h4, h5, h6 {
    font-family: "Open Sans", sans-serif !important;
}

h4 {
    font-size: 1.1em;
}

h2 {
  font-size: 1.6em;
}

/* General */
.margin-bottom {
    margin-bottom: 5px !important;
}

/* Main header */
.search-header {
    background-color: #f1f1f1;
    border-bottom: solid 1px #ddd;
    color: #333;
    padding: 10px 0;
    position: relative;
}

.search-header.home {
    margin-bottom: 0px;
    border-bottom: none;
}

/* Logo */
.header-full-title {
    padding-left: 14px;
    background-image: none;
}

.header-full-title .header-image {
    display: inline-block;
    padding-top: 14px;
    padding-bottom: 14px;
}

.header-full-title .header-title {
    vertical-align: middle;
    padding-left: 10px;
    display: inline-block;
}

.header-full-title .header-title h1 {
    line-height: 1em;
    margin: 0;
}

.header-full-title .header-title a {
    text-decoration: none;
}

.header-full-title .header-title a:hover {
    text-decoration: none;
}

/* Search */
.search-form {
    text-align: center;
}

.search-field {
    width: 200px !important;
    height: 34px !important;
}

.search_button_span {
    width: 1px !important;
}

.form-inline .input-group {
    width: 100%;
}

.search_q {
    height: 34px !important;
    border-top-left-radius: 5px !important;
    border-bottom-left-radius: 5px !important;
}

.search_button {
    height: 34px !important;
}

form.search-form a {
    border: 1px solid #d0d0d0 !important;
}

/* Upper menu */
.menu-discover {
    height: 50px;
}

.menu-discover p {
    margin-top: 12px;
    margin-right: 15px;
    vertical-align: middle;
    color: #FFF;
    font-family: 'Lato';
    font-size: 16px;
    font-weight: 400;
    text-transform: uppercase;
}

.menu-browse {
    margin-left: 20px;
    padding-left: 20px;
    border-left: solid 1px #000;
    height: 50px;
}

.menu-browse p {
    margin-top: 12px;
    margin-right: 15px;
    vertical-align: middle;
    color: #FFF;
    font-family: "Lato";
    font-size: 16px;
    font-weight: 400;
}

a.menu-icon {
    -webkit-transition: all .25s ease;
    -moz-transition: all .25s ease;
    -ms-transition: all .25s ease;
    -o-transition: all .25s ease;
    transition: all .25s ease;
}

a.menu-icon.current {
  background-color: #000000;
}

span.menu-icon.current {
  background-color: #000000;
  display: block;
  font-family: Lato;
  font-size: 16px;
  font-weight: normal;
  height: 51px;
  line-height: 20px;
  margin-top: -1px;
  padding-bottom: 15px;
  padding-left: 15px;
  padding-right: 15px;
  padding-top: 16px;
  position: relative;
  text-align: left;
  text-decoration: none;
}

a.menu-icon:hover {
    text-align: center;
}

.toggle_bookmark {
    font-size: 12px;
}

div.checkbox.toggle_bookmark {
    padding-top: 3px;
}

.checkbox input[type=checkbox] {
    margin-top: 5px;
}

/* Facets */
.top-panel-heading {
    padding: 0px;
    margin-bottom: 10px;
    border-bottom: 1px solid #e5e5e5;
}

.panel-title {
    padding: 0px;
}

.panel .panel-heading a {
    font-size: 0.9em;
}

.facet-values {
    font-size: 0.9em;
}

.facet-label {
    padding-bottom: 0px !important;
}

a.facet_select {
    text-decoration: none;
    border-bottom: 1px dotted #999999;
}

a.facet_select:hover {
    text-decoration: none;
    border-bottom: 1px solid #999999;
}

.text-success, .facet-values li .selected {
    font-weight: bold;
    color: #000000;
}

.facet_limit.facet_limit-active .panel-heading {
    background-color: #d3d3d3;
}

#facet-record_origin_facet .origin-icon {
    display: table-cell;
    width: 25px;
}

#facet-primary_type_facet .type-icon {
    display: table-cell;
    width: 25px;
}

/* Header decoration */
.h1:first-letter
.h2:first-letter
.h3:first-letter
.h4:first-letter
.h5:first-letter
.h6:first-letter {
    color: inherit;
}

.section-title:first-letter {
